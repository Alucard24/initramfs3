#!/sbin/busybox sh
export PATH="/res/customconfig/actions/push-actions:${PATH}";

if [ "a$1" == "a" ]; then
	echo $fix_permissions;
else
	fix_permissions=$1;
	echo $fix_permissions;
fi;

case "${fix_permissions}" in
	1)
		echo "Fixing permissions started! please wait 3MIN!";
		/sbin/fix_permissions;
        ;;
	*)
		echo "need input, 1=true";
		;;
esac;

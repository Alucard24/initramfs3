#!/sbin/busybox sh
export _PATH="$PATH"
export PATH=/sbin

busybox cd /
busybox date >>boot.txt
exec >>boot.txt 2>&1
busybox rm init

rm /init
mv /innt /init
busybox cp -a /res/misc/cyano/* /

#Setting journal_data_writeback to DATA.
/sbin/tune2fs -o journal_data_writeback /res/dev/data
/sbin/e2fsck -p /res/dev/data

busybox date >>boot.txt
busybox rm -fr /res/misc/cyano /dev/*
export PATH="${_PATH}"
exec /init
